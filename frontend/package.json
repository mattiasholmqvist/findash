    "react-window": "^1.8.8",
    "react-window-infinite-loader": "^1.0.9",
    "uuid": "^9.0.1",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.7",
    "@types/react-window": "^1.8.8",
